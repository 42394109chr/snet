Imports SIGDATA

Public Class clslogin

    Function selaccusr(ByVal user As String, ByVal clav As String) As DataTable

        Dim trans As New clslog
        Return trans.selaccusr(user, clav)

    End Function

    Function selmenusr(ByVal usuario As String, clave As String, proceso As String) As DataTable

        Dim trans As New clslog
        Return trans.selmenusr(usuario, clave, proceso)

    End Function


End Class
